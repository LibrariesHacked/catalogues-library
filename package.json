{
  "name": "catalogues-library",
  "version": "2.0.1",
  "description": "A JS library for searching library catalogues",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "cross-env NODE_OPTIONS=\"${NODE_OPTIONS} --experimental-vm-modules\" jest --runInBand",
    "test-ci": "cross-env NODE_OPTIONS=\"${NODE_OPTIONS} --experimental-vm-modules\" jest --silent",
    "test-file": "cross-env NODE_OPTIONS=\"${NODE_OPTIONS} --experimental-vm-modules\" jest --no-color 2>output.txt"
  },
  "jest": {
    "transform": {},
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LibrariesHacked/catalogues-library.git"
  },
  "author": "Libraries Hacked",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LibrariesHacked/catalogues-library/issues"
  },
  "homepage": "https://github.com/LibrariesHacked/catalogues-library#readme",
  "devDependencies": {
    "cross-env": "^10.1.0",
    "jest": "^29.7.0"
  },
  "dependencies": {
    "@small-tech/syswide-cas": "^6.0.2",
    "async": "^3.2.6",
    "cheerio": "^1.2.0",
    "cycletls": "^2.0.5",
    "superagent": "^10.3.0",
    "user-agents": "^1.1.669",
    "uuid": "^13.0.0",
    "xml2js": "^0.6.2"
  }
}
